Brain should be defined by default\nbrain Brain;\n\n\n// START V5 MACROS\n#define waitUntil(condition)                                                   \\\n  do {                                                                         \\\n    wait(5, msec);                                                             \\\n  } while (!(condition))\n\n#define repeat(iterations)                                                     \\\n  for (int iterator = 0; iterator < iterations; iterator++)\n// END V5 MACROS\n\n\n// Robot configuration code.\nmotor LeftDriveSmart = motor(PORT1, ratio18_1, false);\nmotor RightDriveSmart = motor(PORT2, ratio18_1, true);\ndrivetrain Drivetrain = drivetrain(LeftDriveSmart, RightDriveSmart, 319.19, 295, 40, mm, 1);\n\n\n\n\n\n\n// Generated code.\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat GridWidth;\n\nevent message1 = event();\nevent Forward = event();\nevent Left = event();\nevent Right = event();\nevent PATH2 = event();\nevent PATH3 = event();\nevent ForwardPath3 = event();\nevent ForwardPath2 = event();\nevent SPIRALPATH = event();\n\n// \"when I receive PATH3\" hat block\nvoid onevent_PATH3_0() {\n  repeat(3) {\n    ForwardPath3.broadcastAndWait();\n    Right.broadcastAndWait();\n    wait(5, msec);\n  }\n}\n\n// \"when I receive PATH2\" hat block\nvoid onevent_PATH2_0() {\n  repeat(4) {\n    ForwardPath2.broadcastAndWait();\n    Left.broadcastAndWait();\n    ForwardPath2.broadcastAndWait();\n    Right.broadcastAndWait();\n    wait(5, msec);\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  PATH3.broadcast();\n  return 0;\n}\n\n// \"when I receive SPIRALPATH\" hat block\nvoid onevent_SPIRALPATH_0() {\n  repeat(3) {\n    ForwardPath3.broadcastAndWait();\n    Right.broadcastAndWait();\n    wait(5, msec);\n  }\n}\n\n// \"when I receive ForwardPath2\" hat block\nvoid onevent_ForwardPath2_0() {\n  GridWidth = 8.0;\n  Drivetrain.driveFor(forward, (GridWidth * 1.5), inches, true);\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"FORWARD\");\n}\n\n// \"when I receive Right\" hat block\nvoid onevent_Right_0() {\n  Drivetrain.turnFor(right, 75.0, degrees, true);\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"RIGHT\");\n}\n\n// \"when I receive ForwardPath3\" hat block\nvoid onevent_ForwardPath3_0() {\n  GridWidth = 8.0;\n  Drivetrain.driveFor(forward, (GridWidth * 6.25), inches, true);\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"FORWARD\");\n}\n\n// \"when I receive Left\" hat block\nvoid onevent_Left_0() {\n  Drivetrain.turnFor(left, 75.0, degrees, true);\n  Brain.Screen.newLine();\n  Brain.Screen.print(\"LEFT\");\n}\n\n\nint main() {\n  // register event handlers\n  PATH3(onevent_PATH3_0);\n  PATH2(onevent_PATH2_0);\n  SPIRALPATH(onevent_SPIRALPATH_0);\n  ForwardPath2(onevent_ForwardPath2_0);\n  Right(onevent_Right_0);\n  ForwardPath3(onevent_ForwardPath3_0);\n  Left(onevent_Left_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}